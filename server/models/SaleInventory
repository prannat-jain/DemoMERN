const mongoose = require("mongoose");
const Schema = mongoose.Schema;

const SaleInventorySchema = new Schema({
  itemText: {
    type: String,
    required: true,
  },
  stillAvailable: {
    type: Boolean,
    default: true,
  },
  timeStamp: {
    type: String,
    default: Date.now(),
  },
});

const SaleInventory = mongoose.model("SaleInventory", SaleInventorySchema);

module.exports = Todo;
